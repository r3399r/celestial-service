{
  "name": "@y-celestial/service",
  "version": "2.1.7",
  "author": "Yue Chang",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint:es": "eslint -c ./eslint.json src",
    "fix:es": "npm run lint:es -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./**/*.ts\" --ignore-path .gitignore",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist lib -f",
    "compile": "ttsc",
    "test": "jest -c jest.config.js",
    "test:coverage": "npm run test -- --coverage --verbose",
    "pre:commit": "run-s lint test",
    "post:publish": "run-s clean compile && npm publish"
  },
  "dependencies": {
    "aws-sdk": "^2.963.0",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "del-cli": "^4.0.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.2",
    "typescript-transform-paths": "^3.3.1"
  },
  "files": [
    "lib/**/*"
  ]
}
