{
  "name": "@y-celestial/service",
  "version": "2.0.2",
  "author": "Yue Chang",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint:ts": "tslint --project .",
    "fix:ts": "npm run lint:ts -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./**/*.ts\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist lib -f",
    "compile": "ttsc",
    "test": "jest -c jest.config.js",
    "test:coverage": "npm run test -- --coverage --verbose",
    "pre:commit": "run-s lint test",
    "post:publish": "run-s clean compile && npm publish"
  },
  "dependencies": {
    "aws-sdk": "^2.963.0",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@types/node": "^15.14.7",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "del-cli": "^3.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.0.0",
    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules-recommended": "^1.2.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.4.4"
  },
  "files": [
    "lib/**/*"
  ]
}
