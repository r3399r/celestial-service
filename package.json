{
  "name": "@y-celestial/service",
  "version": "1.5.9",
  "author": "Yue Chang",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint:ts": "tslint --project .",
    "fix:ts": "npm run lint:ts -- --fix",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./{src,test}/**/*.ts\"",
    "fix:format": "npm run lint:format -- --write",
    "lint": "run-p lint:*",
    "fix": "run-s fix:*",
    "clean": "del-cli dist lib -f",
    "build": "webpack --config webpack.config.js",
    "compile": "ttsc",
    "test": "jest -c jest.config.js",
    "test:coverage": "npm run test -- --coverage --verbose",
    "test:lambda": "npm run build:lambda && sam local invoke -t aws/cloudformation/template.yaml -e test/event.json LambdaHelpMe",
    "pre:commit": "run-s lint test",
    "pre:deploy": "run-s clean build",
    "post:publish": "run-s clean compile && npm publish"
  },
  "dependencies": {
    "@line/bot-sdk": "^7.2.0",
    "aws-sdk": "^2.963.0",
    "axios": "^0.21.1",
    "google-spreadsheet": "^3.1.15",
    "inversify": "^6.0.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/google-spreadsheet": "^3.1.5",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/node": "^15.14.7",
    "@types/uuid": "^8.3.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "del-cli": "^3.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.0.0",
    "express": "^4.17.1",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.5",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules-recommended": "^1.2.2",
    "tslint-microsoft-contrib": "^6.2.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.4.4",
    "webpack": "^5.49.0",
    "webpack-cli": "^4.7.2",
    "zip-webpack-plugin": "^4.0.1"
  },
  "files": [
    "lib/**/*"
  ]
}
